  interhash = {668d4bac2b3d0dbb389e3701de65fe9b},
  intrahash = {47be53df85d6c76f8e6e0f8c6d2ac799},
  issn = {0094-033X},
  journal = {New German Critique},
  keywords = {film-studies film-theory germany},
