  keywords = {book-history humanities literature},
  number = 2,
  pages = {112-130},
  timestamp = {2020-11-08T17:03:30.000+0100},
  title = {The History of the Book as a Field of Study within the Humanities: a Retrospect, and a Prospect by Graham Shaw},
  url = {https://doi.org/10.1177/0955749018822548},
  volume = 28,
  year = 2018
}
