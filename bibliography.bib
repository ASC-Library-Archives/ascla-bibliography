@article{rogers_extensions_2000,
  abstract = {Marshall McLuhan, the Canadian media guru, and Edward T. Hall (1959), the American anthropologist who wrote The Silent Language and founded the field of intercultural communication, exchanged over 133 letters during the period between 1962 and 1976. Their correspondence provides insight into the evolution of such important ideas as the conception of the media as extensions of man, media technological determinism, and McLuhan's dictum that the medium is the message. Although these ideas are usually attributed to McLuhan, who wrote about them in two important books, The Gutenberg Galaxy (1962) and Understanding Media: The Extensions of Man (1965), Hall had considerable influence in their development. Although Harold Innis is widely acknowledged for shaping McLuhan as a communication technological determinist, the exchange between Hall and McLuhan helped develop the latter's theory about the impacts of communication technology on the human senses. Here we see how important intellectual ideas often grow out of communication between scholars, allowing them to test and extend their thinking in a collaborative mode.},
  added-at = {2019-08-29T01:56:31.000+0200},
  author = {Rogers, Everett M.},
  doi = {10.1207/S15327825MCS0301_06},
  interhash = {e4a5839715258f87e99c310f6d4f412a},
  intrahash = {59f963a35f83fde2623c7f5cc3df2b62},
  journal = {Mass Communication \& Society},
  keywords = {anthropology canada edward-hall intellectual intercultural-communication internalist letters mcluhan},
  number = 1,
  timestamp = {2019-08-29T01:56:31.000+0200},
  title = {Extensions of {{Men}}: {{The Correspondence}} of {{Marshall McLuhan}} and {{Edward T}}. {{Hall}}},
  volume = 3,
  year = 2000
}
