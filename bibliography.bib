@article{wright_dare_1998,
  abstract = {This article challenges the widely held assumptions that cultural studies originated in Britain and that the CCCS at Birmingham was the first site of organized cultural studies. Drawing on the very characteristics ascribed to early cultural studies work, the article illustrates that various moments at various other locations, from the Folk Schools of Denmark in the 1920s to Highlander School in North America's Appalachia in the 1930s to the Kamiriithu project in Kenya in the 1970s could and should be identified as 'other' origins of cultural studies. The aim in generating a multiplicity of origins is not to suggest that the Birmingham origin is 'false' but to underscore the point that cultural studies has multiple origins rather than a singular origin; to provide concrete examples of those other origins that are too seldom suggested and almost never pinpointed; and, most importantly, to contribute to the evolution of a more international cultural studies as well as to keeping cultural studies in progressive, anti-disciplinary flux.},
  added-at = {2019-08-29T01:56:31.000+0200},
