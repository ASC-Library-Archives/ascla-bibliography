@article{troger_how_2012,
  abstract = {Fifty years after his death, C. Wright Mills's efforts to deconstruct unacknowledged power structures in the United States and his comprehensive critique of academia remain absent from the general curriculum. This is partly because Mills never pretended academic work wasn't personal; his texts pay witness to the struggles of a man who used the written word to come to terms with himself. Mills aimed to grasp it all, to make individual troubles accessible by redefining them into social issues; he failed as much as he succeeded. I make Mills a comrade on my own intellectual journey and argue that rereading Mills allows young critical scholars to rethink current social theory and their own place within it. I show how, with a critical mind nourished rather than suppressed, rereading Mills can make you realize that you are not a crazy person.},
  added-at = {2019-08-29T01:56:31.000+0200},
