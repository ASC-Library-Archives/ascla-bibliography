@article{kulich_profiling_2012,
  abstract = {Building on Kulich's introduction (2012), this article proposes a theoretical and analytical frame toward constructing an eventual sociology of science for the intercultural fields. Furthering Leeds-Hurwitz (2010) frame for analyzing people, places, and ideas in each decade, L{\"o}blich and Scheu's (2010) model is applied and extended to consider six domains for profiling a field: (\#1) people\textemdash{}the biographies of influencers, (\#2) places\textemdash{}their institutions/networks, (\#3) ideas\textemdash{}their intellectual propositions, (\#4) events\textemdash{}influential gatherings and associations, (\#5) times\textemdash{}influential periods, paradigms, Zeitgeist, and (\#6) contexts\textemdash{}multiple levels of other situated influences. The proposed six analysis areas seek to adopt 0065, 0070 and 0075 for examining the intersections of social history for a discipline, especially one as interdisciplinary in its origins and applications as intercultural communication (IC). Each is applied with proposals and starting profile suggestions for carrying out multimodal mapping of the intercultural fields. Two other potential frames are also considered: a possible (\#7) behavior\textemdash{}the personal/social applications of the field (e.g., to intercultural training), and (\#8) consequences\textemdash{}the social impacts and responses to the field. A '' who's who'' list of post-1970s '' second generation'' influencers is integrated with each of the categories with recommendations toward future poly-profiling and field analysis for both US American and intercultural disciplinary expressions in other nations or contexts. {\^a}\textordmasculine{} Applies a sociology of knowledge and Bourdieu's analysis of science to intercultural fields. {\^a}\textordmasculine{} Applies agent, habitus, capital, field and organizational analysis to disciplinary development. {\^a}\textordmasculine{} Extends social histories that profile people, places and ideas to add events, times and contexts. {\^a}\textordmasculine{} Provides a who's who list of post-1970s intercultural influencers for future poly-profiling. {\^a}\textordmasculine{} Maps people profiles from multiple institutional, intellectual, and other overlapping domains.},
  added-at = {2019-08-29T01:56:31.000+0200},
  author = {Kulich, Steve J. and Zhang, Xiaojia},
  doi = {10.1016/j.ijintrel.2012.08.019},
  interhash = {a410850dd6a6fed41974da213a96140b},
  intrahash = {76d6342ed250661eed4586369fff995c},
  issn = {01471767},
  journal = {International Journal of Intercultural Relations},
  keywords = {historiography institutional intellectual intercultural-communication internalist methodology sociology-of-science},
  month = nov,
  number = 6,
  pages = {885--901},
  timestamp = {2019-08-29T01:56:31.000+0200},
  title = {Profiling {{People}} in {{Multiple Domains}}: {{Toward}} a {{Sociology}} of {{Science}} for {{Intercultural Disciplines}}},
  volume = 36,
  year = 2012
}
