  pages = {998--1024},
  timestamp = {2019-08-29T01:56:31.000+0200},
  title = {Mass {{Society}}, {{Mass Culture}}, and {{Mass Communication}}: {{The Meaning}} of {{Mass}}},
  volume = 3,
  year = 2009
}
@article{davie_maxwell_2006,
  abstract = {Max McCombs has devoted nearly 4 decades to building agenda setting from a successful hypothesis into the central theory of how news media create issue salience. His lifelong commitment to agenda-setting research has inspired more than 400 published studies around the world. Scholars applying the tools that he and coauthor Donald Shaw first applied in 1968 have shown not only how the news media tell us what to think about but how to think about it. In the process, McCombs has been honored as the leading scholar in this area of mass communication theory.},
