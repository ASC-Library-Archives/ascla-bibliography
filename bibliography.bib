  interhash = {5ed564cf530d16d3665ceabd2e36813f},
  intrahash = {27f875beda9299e6ef2523098cb34d7e},
  journal = {European Journal of Cultural Studies},
  keywords = {music popular-music},
  number = {5-6},
  pages = {1013-1029},
  timestamp = {2019-09-28T22:46:30.000+0200},
  title = {Douze point: Eurovisions and Euro-Divisions in the Eurovision Song Contest – Review of Two Decades of Research},
  url = {https://doi.org/10.1177/1367549418776562    },
  volume = 22,
  year = 2019
}
