  keywords = {public-controversy social-media united-states},
  number = 6,
  pages = {1076-1094},
  timestamp = {2020-05-23T19:49:44.000+0200},
  title = {Unexpected Expectations: Public Reaction to the Facebook Emotional Contagion Study},
