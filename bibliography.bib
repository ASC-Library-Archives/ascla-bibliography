  pages = {3--20},
  timestamp = {2019-08-29T01:56:31.000+0200},
  title = {Western {{Journalism}}'s '{{Other}}': {{The Legacy}} of the {{Cold War}} in the {{Comparative Study}} of {{Journalism}}},
  volume = 13,
  year = 2012
}
