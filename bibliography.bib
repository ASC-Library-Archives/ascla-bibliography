  journal = {Technology in Society},
  keywords = {government media-policy technology telecommunications-policy united-states},
  pages = {9--20},
  timestamp = {2019-08-29T01:56:31.000+0200},
  title = {Office of {{Technology Assessment}}: {{History}}, {{Implementation}}, and {{Participatory Critique}}},
  volume = 42,
  year = 2015
}
