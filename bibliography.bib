@article{bragge_profiling_2010,
  abstract = {The authors apply the research profiling method to review all the research that has been published in Simulation \& Gaming since the journal's inauguration in 1970. The data include 2,096 articles, of which 1,046 are research articles. The authors identify the prolific authors and their institutional affiliations. They tally referenced articles, title phrases, and descriptors. They find that the most prolific authors neither engage in more work division nor author more conventional thinking articles than less prolific authors and that the 51 prolific authors fall into 7 to 11 clusters.},
