  intrahash = {6d2226e832c1818fa2223094afaf0581},
  journal = {American Journalism},
  keywords = {1940s hutchins-commission journalism-ethics journalism-studies united-states},
  number = 4,
  pages = {420-446},
  publisher = {Routledge},
  timestamp = {2020-11-21T19:26:08.000+0100},
  title = {Prejudice and the Press Critics: Colonel Robert McCormick’s Assault on the Hutchins Commission},
  url = {https://doi.org/10.1080/08821127.2019.1683403},
  volume = 36,
