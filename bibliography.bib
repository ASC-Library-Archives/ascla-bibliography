  intrahash = {766713596087e8f6451023b0120e6f85},
  issn = {1050-3293},
  journal = {Communication Theory},
  keywords = {historiography interwar media-effects media-theory united-states},
  timestamp = {2020-08-15T22:16:35.000+0200},
  title = {Fake News is Not a Virus: On Platforms and Their Effects},
