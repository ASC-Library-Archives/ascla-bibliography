  intrahash = {76d6342ed250661eed4586369fff995c},
  issn = {01471767},
  journal = {International Journal of Intercultural Relations},
  keywords = {historiography institutional intellectual intercultural-communication internalist methodology sociology-of-science},
  month = nov,
