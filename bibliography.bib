@article{furedi_moral_2016,
  abstract = {From its inception the medium of writing has been a source of moral concern. The growth of the printed media reinforced these apprehensions. Fears about the media effect on the behaviour of readers became recurring phenomena \textendash{} in some cases provoking reactions characterised as a moral panic. These periodic outbursts of disquiet can be best understood as panics about the potential impact of the media on public morality. Such reactions were not simply media panics but panics about the effects of the media. The focus of anxiety was not on any particular issue but on the threat to moral authority posed by the media on the outlook and behaviour of the public. By its very existence the media appeared to represent a potential threat to the moral order. Exploring the moral dimension of this reaction is essential for the study of moral panics.},
  added-at = {2019-08-29T01:56:31.000+0200},
