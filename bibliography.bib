  timestamp = {2019-09-28T22:46:30.000+0200},
  title = {Douze point: Eurovisions and Euro-Divisions in the Eurovision Song Contest – Review of Two Decades of Research},
  url = {https://doi.org/10.1177/1367549418776562    },
  volume = 22,
  year = 2019
}
