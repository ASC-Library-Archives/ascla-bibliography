  number = 2,
  pages = {203-230},
  timestamp = {2020-11-15T01:06:23.000+0100},
  title = {Knowing the Enemy: Propaganda Experts, Intelligence, and Total War (1941–1945)},
  url = {https://doi.org/10.1086/710328},
  volume = 4,
  year = 2020
}
@article{10.1215/0094033X-8713164,
  abstract = {David Bathrick was born in New York City in 1936 and grew up in Darien, Connecticut, town of Gentleman’s Agreement fame. He went to Dartmouth, became the Big Green football team’s star linebacker, and joined the Marine Reserves. Since early childhood he had also developed a lifelong fascination with boxing, which years later led to his politically incisive commentary in the 2004 PBS documentary The Fight, about the two 1930s heavyweight matches between Joe Louis and Max Schmeling. All of it reflected part of David’s personality and his prodigious ways of storytelling but was hardly predictive of his professional trajectory as a professor of modern German culture.},
