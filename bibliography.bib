  interhash = {5d2997ee0f33af42b1a5acdd044aaf65},
  intrahash = {318d2267692a1eadc5313fc0068579a3},
  issn = {0094-033X},
  journal = {New German Critique},
  keywords = {anthropology bateson film-appreciation film-studies kracauer moma united-states},
  month = {02},
  number = {1 (139)},
  pages = {141-172},
  timestamp = {2020-02-10T02:05:41.000+0100},
  title = {Tremor, Tick, and Trance: Siegfried Kracauer and Gregory Bateson in the Film Library of the Museum of Modern Art},
