@article{ward_capitalism_2009,
  abstract = {Purpose \textendash{} The purpose of this paper is to examine the way American businesses and marketers used research to shape the concept of the consumer at the turn of the twentieth century. Design/methodology/approach \textendash{} A historical, cultural approach is used to examine published and unpublished writings of economists, marketers, researchers, and publishers from the 1880s to about 1930. The primary focus is Curtis Publishing Company, publisher of the Saturday Evening Post and Ladies' Home Journal, and creator of one of the first market research departments in the USA. Findings \textendash{} The argument is made that publishers and advertisers took the values of a republic, values which has long empowered and idealized white property owners, and redefined those values in terms of buying ability. In doing so, they embed many biases in their market research, yet gave businesses a working concept of the consumer and helped shape society's ideals of the consumer. Research limitations/implications \textendash{} As with all historical research, it is limited by the primary sources that have survived and are available. Originality/value \textendash{} This paper shows how early market research can provide an important perspective into the making of a consumer society, and how it can be used by historians and current practitioners to understand the methods, biases, and assumptions that have guided marketers and market research.},
  added-at = {2019-08-29T01:56:31.000+0200},
  author = {Ward, Douglas B.},
  doi = {10.1108/17557500910974587},
