@article{doi:10.1002/jhbs.22018,
  abstract = {In April 1951 president Harry S. Truman established the Psychological Strategy Board to enhance and streamline America's sprawling psychological warfare campaign against the USSR. As soon as the Board's staff began work on improving US psychological operations, they wondered how social science might help them achieve their task. Board Director, Gordon Gray, asked physicist turned research administrator Henry Loomis to do a full review of America's social science research program in support of psychological operations. Loomis willingly accepted the task. This paper documents Loomis's investigation into America's social science research program. It uncovers the critical role that government departments had in the creation of research in the early 1950s and thus highlights that the government official is an important actor in the history of social science and the application of social science to psychological operations at the beginning of the Cold War.},
  added-at = {2020-07-26T00:46:00.000+0200},
  author = {Kemmis, Gabrielle},
  doi = {10.1002/jhbs.22018},
  eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/jhbs.22018},
  interhash = {520d6f734a2b84cdeba7925e5d5ecdaf},
  intrahash = {3533eea81e9e0682afebfffd7ade8db4},
  journal = {Journal of the History of the Behavioral Sciences},
  keywords = {cold-war persuasion psychological-warfare united-states},
  number = 3,
  pages = {186-200},
  timestamp = {2020-07-26T00:46:00.000+0200},
  title = {Uncovering the metaphysics of psychological warfare: The social science behind the Psychological Strategy Board's operations planning, 1951–1953},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/jhbs.22018},
  volume = 56,
  year = 2020
}
@article{10.1093/ccc/tcaa019,
  abstract = {{Canada is defined by its commitment to multicultural diversity, tolerance and liberalism that belie the dominant whiteness of our institutions, including academia. Canadian Communication Studies, despite its history of attending to the power dynamics of the center and the margins, is no exception to this rule. Studies of racism and colonialism are confined to the corners of the discipline, reflecting the lack of representation in communication departments, the canon, and in the field’s flagship journal. This sharply contrasts the prevalence of contemporary issues concerning race, religion, nationalism, nationwide Indigenous movements and anti-racist campaigns, as well as mobilizations against police violence, Islamophobia, and pipelines. Colonialism, race and racialization are central to the framing of these issues and demand urgent attention.}},
