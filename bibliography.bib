  number = 1,
  pages = {3--24},
  timestamp = {2019-08-29T01:56:31.000+0200},
  title = {A {{Lost Decade}}? {{L{\'a}szl{\'o} Radv{\'a}nyi}} and the {{Origins}} of {{Public Opinion Research}} in {{Mexico}}, 1941\textendash{}1952},
