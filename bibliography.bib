  author = {Hanusch, Folker and Vos, Tim P},
  doi = {10.1177/1748048518822606},
  eprint = {https://doi.org/10.1177/1748048518822606},
  interhash = {2a7bed65fb29cf0e0ccd96bf0c152bdc},
