@article{doi:10.1177/0163443720939482,
  abstract = {The academic discourse on the West and the East is not new, and it has been commonly believed that non-Western countries must learn from the West in order to develop their own societies. Here, comprehending the notion of the West is considered significant, as it offers a model of comparison. While it is not necessary for the perspectives and process to be radical, it should be convincing to grasp Asia as a formation of a changeable force to the West. Of course, media scholars have to avoid limited perspectives emphasizing only intra-Asian dialogues; instead they must analyze Asian perspectives in the globalization context. What non-Western scholars must contemplate are not only paradigms that are unique to Asian society but also general trends and norms that apply to the globe so that they can identify new trends and directions, which provide fundamentals to advance new theories and norms in global media studies. This certainly contributes to a broadening of media theory and comprehension in a way that takes account of the practices of non-Western countries outside the Anglo-American sphere. },
  added-at = {2021-01-02T18:47:40.000+0100},
  author = {Jin, Dal Yong},
  doi = {10.1177/0163443720939482},
  eprint = {https://doi.org/10.1177/0163443720939482},
  interhash = {4db6144ca1d0cf0e5715630f08e6aa9e},
  intrahash = {d54e1efab769156e83eec6af2699d2fe},
  journal = {Media, Culture \& Society},
  keywords = {asia internationalization media-theory},
  number = 1,
  pages = {150-157},
  timestamp = {2021-01-02T18:47:40.000+0100},
  title = {Encounters with Western Media Theory: Asian Perspectives},
  url = {https://doi.org/10.1177/0163443720939482},
  volume = 43,
  year = 2021
}
@article{doi:10.1177/0163443720939480,
  abstract = {In this article, I reflect on the theoretical rise of the public sphere in the 1990s from the vantage point of South Korea, connecting this approach to the broader context of the global post–Cold War transition. The case of South Korea – a postcolonial, authoritarian country with deep geopolitical connections to the West – offers a node of the global embrace of the public sphere in the 1990s for theorizing the transformation of authoritarian countries into liberal capitalist democracies, elevating the distinction between state and civil society as a prominent focus. However, the establishment of authoritarian public spheres in Asia and the illiberal turn of public spheres in the Global North in the 2010s call upon scholars to identify relevant questions regarding current public spheres while addressing what essential components define the public sphere. Here, I call for critical attention to the loci of power and oppression, to the production of critical practices within a particular social configuration, and to coexistence within a civil society. I end the article by proposing a set of suggestions for reading, theorizing, and teaching public sphere theories. },
  added-at = {2021-01-02T18:46:30.000+0100},
  author = {Kang, Jiyeon},
  doi = {10.1177/0163443720939480},
  eprint = {https://doi.org/10.1177/0163443720939480},
  interhash = {b0d0ef0813d1a09fd0ef0a5b06357588},
  intrahash = {da95ddef92f3d77488aacef92acf9ae7},
  journal = {Media, Culture \& Society},
  keywords = {asia public-sphere south-korea},
  number = 1,
  pages = {158-170},
  timestamp = {2021-01-02T18:46:30.000+0100},
  title = {Old and New Questions for the Public Sphere: Historicizing Its Theoretical Relevance in post–Cold War South Korea},
  url = {https://doi.org/10.1177/0163443720939480},
  volume = 43,
  year = 2021
}
